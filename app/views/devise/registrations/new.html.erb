<div class="login" >
  <div class="sign">
<h2>Sign Up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div>
     <%= f.text_field :first_name, autofocus: true , placeholder: "First Name"%></div>
    <div>
      <%= f.text_field :last_name, placeholder: "Last Name" %></div>

  <div>
  <%= f.email_field :email, autofocus: true , placeholder: "Email Address"%></div>

  <div><% if @validatable %><i>(<%= @minimum_password_length %> characters minimum)</i><% end %>
    <%= f.password_field :password, autocomplete: "off" , placeholder: "Password"%></div>

  <div>
    <%= f.password_field :password_confirmation, autocomplete: "off" , placeholder: "Confirm Password"%></div>

  <div><%= f.submit "Sign up", :class => "btn btn-primary btn-lg" %></div>
<% end %>

<%= link_to "Login", new_manager_session_path, method: :get %>
 </div></div>
